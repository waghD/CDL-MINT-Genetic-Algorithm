<?xml version="1.0" encoding="UTF-8"?>
<eMISA_Xtend:System
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:eMISA_Xtend="http://www.example.org/eMISA_Xtend"
    xsi:schemaLocation="http://www.example.org/eMISA_Xtend eMISA_Xtend.ecore"
    name="AutomatedSystem">
  <block name="roboticArm">
    <statemachine
        name="Workflow">
      <state
          name="Idle"
          incoming="//@block.0/@statemachine/@transition.7"
          outgoing="//@block.0/@statemachine/@transition.0"
          isInitial="true">
        <assignment
            property="//@block.0/@property.2"/>
        <assignment
            property="//@block.0/@property.0"/>
        <assignment
            property="//@block.0/@property.1"/>
        <assignment
            property="//@block.0/@property.3"/>
        <assignment
            property="//@block.0/@property.4"/>
      </state>
      <state
          name="Start"
          incoming="//@block.0/@statemachine/@transition.0"
          outgoing="//@block.0/@statemachine/@transition.1">
        <assignment
            property="//@block.0/@property.2"/>
        <assignment
            property="//@block.0/@property.0"/>
        <assignment
            property="//@block.0/@property.1"
            value="0.1"/>
        <assignment
            property="//@block.0/@property.3"/>
        <assignment
            property="//@block.0/@property.4"/>
      </state>
      <state
          name="Adjust"
          incoming="//@block.0/@statemachine/@transition.1"
          outgoing="//@block.0/@statemachine/@transition.2">
        <assignment
            property="//@block.0/@property.2"/>
        <assignment
            property="//@block.0/@property.0"
            value="1.0"/>
        <assignment
            property="//@block.0/@property.1"
            value="0.2"/>
        <assignment
            property="//@block.0/@property.3"/>
        <assignment
            property="//@block.0/@property.4"/>
      </state>
      <state
          name="DriveDown"
          incoming="//@block.0/@statemachine/@transition.2"
          outgoing="//@block.0/@statemachine/@transition.3"
          isInitial="true">
        <assignment
            property="//@block.0/@property.2"
            value="1.5"/>
        <assignment
            property="//@block.0/@property.0"
            value="1.6"/>
        <assignment
            property="//@block.0/@property.1"
            value="1.5"/>
        <assignment
            property="//@block.0/@property.3"
            value="-0.12"/>
        <assignment
            property="//@block.0/@property.4"/>
      </state>
      <state
          name="PickUp"
          incoming="//@block.0/@statemachine/@transition.3"
          outgoing="//@block.0/@statemachine/@transition.4">
        <assignment
            property="//@block.0/@property.2"
            value="-0.4"/>
        <assignment
            property="//@block.0/@property.0"
            value="1.6"/>
        <assignment
            property="//@block.0/@property.1"
            value="1.5"/>
        <assignment
            property="//@block.0/@property.3"
            value="-0.12"/>
        <assignment
            property="//@block.0/@property.4"/>
      </state>
      <state
          name="Lift"
          incoming="//@block.0/@statemachine/@transition.4"
          outgoing="//@block.0/@statemachine/@transition.5">
        <assignment
            property="//@block.0/@property.2"
            value="-0.4"/>
        <assignment
            property="//@block.0/@property.0"
            value="1.6"/>
        <assignment
            property="//@block.0/@property.1"
            value="1.315"/>
        <assignment
            property="//@block.0/@property.3"
            value="-0.12"/>
        <assignment
            property="//@block.0/@property.4"/>
      </state>
      <state
          name="Forwarding"
          incoming="//@block.0/@statemachine/@transition.5"
          outgoing="//@block.0/@statemachine/@transition.6">
        <assignment
            property="//@block.0/@property.2"
            value="-0.4"/>
        <assignment
            property="//@block.0/@property.0"
            value="3.1"/>
        <assignment
            property="//@block.0/@property.1"
            value="1.4"/>
        <assignment
            property="//@block.0/@property.3"
            value="-1.55"/>
        <assignment
            property="//@block.0/@property.4"
            value="-1.5"/>
      </state>
      <state
          name="Release"
          incoming="//@block.0/@statemachine/@transition.6"
          outgoing="//@block.0/@statemachine/@transition.7">
        <assignment
            property="//@block.0/@property.2"
            value="1.48"/>
        <assignment
            property="//@block.0/@property.0"
            value="3.1"/>
        <assignment
            property="//@block.0/@property.1"
            value="1.4"/>
        <assignment
            property="//@block.0/@property.3"
            value="-1.55"/>
        <assignment
            property="//@block.0/@property.4"
            value="-1.5"/>
      </state>
      <transition
          successor="//@block.0/@statemachine/@state.1"
          predecessor="//@block.0/@statemachine/@state.0"/>
      <transition
          successor="//@block.0/@statemachine/@state.2"
          predecessor="//@block.0/@statemachine/@state.1"/>
      <transition
          successor="//@block.0/@statemachine/@state.3"
          predecessor="//@block.0/@statemachine/@state.2"/>
      <transition
          successor="//@block.0/@statemachine/@state.4"
          predecessor="//@block.0/@statemachine/@state.3"/>
      <transition
          successor="//@block.0/@statemachine/@state.5"
          predecessor="//@block.0/@statemachine/@state.4"/>
      <transition
          successor="//@block.0/@statemachine/@state.6"
          predecessor="//@block.0/@statemachine/@state.5"/>
      <transition
          successor="//@block.0/@statemachine/@state.7"
          predecessor="//@block.0/@statemachine/@state.6"/>
      <transition
          successor="//@block.0/@statemachine/@state.0"
          predecessor="//@block.0/@statemachine/@state.7"/>
    </statemachine>
    <property
        name="bp">
      <tolerance
          value="0.01"/>
    </property>
    <property
        name="map">
      <tolerance
          value="0.01"/>
    </property>
    <property
        name="gp">
      <tolerance
          value="0.01"/>
    </property>
    <property
        name="sap">
      <tolerance
          value="0.01"/>
    </property>
    <property
        name="wp">
      <tolerance
          value="0.01"/>
    </property>
  </block>
</eMISA_Xtend:System>
